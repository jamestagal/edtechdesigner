{
	"build": "public",
	"baseurl": "/",
	"theme": "compendium",
	"theme_config": {
		"compendium": {
			"url": "https://github.com/plenti-themes/compendium",
			"commit": "c876ea426b84224e8d4dbfa730f7988325a9be1d"
		}
	},
	"local": {
		"port": 3000
	},
	"routes": {
		"404": ":filename",
		"catgs": ":filename/:paginate(totalCatgsPages)",
		"index": ":paginate(totalPages)",
		"pages": "pages/:filename",
		"posts": "post/:fields(title)",
		"projs": "posts/:paginate(totalProjPages)",
		"tags": ":filename/:paginate(totalTagsPages)"
	}
}