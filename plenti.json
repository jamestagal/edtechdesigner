{
	"build": "deploy/public",
	"baseurl": "/",
	"theme": "compendium",
	"theme_config": {
		"compendium": {
			"url": "https://github.com/plenti-themes/compendium",
			"commit": "7dd5f507ca8785b83d4bf2ef63cd9125a6a1dc42",
			"exclude": [
				"content"
			]
		}
	},
	"local": {
		"port": 3000
	},
	"routes": {
		"404": ":filename",
		"catgs": ":fields(route)",
		"index": ":paginate(totalPages)",
		"pages": "pages/:filename",
		"posts": "post/:fields(title)",
		"projs": ":fields(route)/:paginate(totalProjPages)",
		"tags": ":fields(route)"
	},
	"cms": {
		"repo": "",
		"redirect_url": "",
		"app_id": "",
		"branch": ""
	}
}