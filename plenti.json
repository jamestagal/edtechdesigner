{
	"build": "deploy/public",
	"baseurl": "/",
	"theme": "compendium",
	"entrypoint": "global/html.svelte",
	"theme_config": {
		"compendium": {
			"url": "https://github.com/plenti-themes/compendium",
			"commit": "d344f1d481416111e026148cacec3c9a40613a4b",
			"exclude": [
				"content"
			]
		}
	},
	"local": {
		"port": 3000
	},
	"routes": {
		"404": ":filename",
		"catgs": ":fields(route)",
		"_index": ":paginate(totalPages)",
		"pages": "pages/:filename",
		"posts": "post/:fields(title)",
		"projs": ":fields(route)/:paginate(totalProjPages)",
		"tags": ":fields(route)"
	},
	"cms": {
		"repo": "",
		"redirect_url": "",
		"app_id": "",
		"branch": ""
	}
}